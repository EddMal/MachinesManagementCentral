<h3>DeviceList</h3>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Device Id</th>
            <th scope="col">Location</th>
            <th scope="col">Date</th>
            <th scope="col">Type</th>
            <th scope="col">Status</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var device in DevisteLst)
        {
            <tr>
                <td scope="col">@device.DeviceId</td>
                <td scope="col">@device.Location</td>
                <td scope="col">@device.Date</td>
                <td scope="col">@device.DeviceType</td>
                <td scope="col">@device.Status</td>
            </tr>
        }
    </tbody>

</table>

